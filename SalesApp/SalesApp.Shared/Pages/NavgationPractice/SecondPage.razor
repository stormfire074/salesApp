@page "/faqmasterdata/detail/{Id}"
@inject NavigationManager Navigation

<h3>FAQ Detail</h3>

<p>Received FAQ ID: @FaqId</p>
<button @onclick="NavigateBack">Back to FAQ Master</button>

@code {
    [Parameter]
    public string Id { get; set; }

    private string FaqId => Id;

    private void NavigateBack()
    {
        Navigation.NavigateTo("/faqmaster");
    }
}
